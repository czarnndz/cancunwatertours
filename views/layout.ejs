<body ng-controller="MainCTL"  ng-cloak >

  <div id="wrap">
    <div id="main" class="clearfix">
      <div id="topBackRepeat">
        <div layout="column">

          <div>
            <%- partial('partials/header') -%>

            <%- partial('partials/register') -%>
            <%- partial('partials/login') -%>
            <% if (meta && meta.addMenu) { %>
              <%- partial('partials/menu-mobile') -%>
            <% } %>
          </div>

          <md-content id="main-content">
            <div layout="row">

              <!--SIDEBAR-->
              <% if (meta && meta.addMenu) { %>
                  <%- partial('partials/sidebar-menu') -%>
              <% } %>
              <!--END SIDEBAR-->

              <!--MAIN CONTENT-->
              <section
                <% if (meta && meta.addMenu) { %>
                  flex flex-md="100" flex-sm="100"
                <% } else { %>
                  flex="100"
                <% } %>
              >
                <%- body %>
              </section>
              <!--END MAIN CONTENT-->

            </div>

          </md-content>

        </div>
      </div><!-- END topBackRepeat -->
    </div><!-- END main -->
  </div><!-- END wrap -->
  <div class="clear"></div>

  <%- partial('partials/footer') -%>

    <!--
